<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true"><shape android:shape="rectangle">
            <stroke android:width="1px" android:color="@color/wifilist_emphasize_color" />

            <solid android:color="@color/wifilist_emphasize_color" />

            <corners android:bottomLeftRadius="@dimen/common_radius" android:bottomRightRadius="@dimen/common_radius" android:topLeftRadius="@dimen/common_radius" android:topRightRadius="@dimen/common_radius" />
        </shape></item>
    <item android:state_enabled="false"><shape android:shape="rectangle">
            <stroke android:width="1px" android:color="@color/wifilist_disable_color" />

            <solid android:color="@color/wifilist_disable_color" />

            <corners android:bottomLeftRadius="@dimen/common_radius" android:bottomRightRadius="@dimen/common_radius" android:topLeftRadius="@dimen/common_radius" android:topRightRadius="@dimen/common_radius" />
        </shape></item>
    <item><shape android:shape="rectangle">
            <stroke android:width="1px" android:color="@color/wifilist_emphasize_color" />

            <solid android:color="@color/wifilist_emphasize_color" />

            <corners android:bottomLeftRadius="@dimen/common_radius" android:bottomRightRadius="@dimen/common_radius" android:topLeftRadius="@dimen/common_radius" android:topRightRadius="@dimen/common_radius" />
        </shape></item>

</selector>