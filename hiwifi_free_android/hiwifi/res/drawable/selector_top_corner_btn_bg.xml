<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <stroke android:width="1px" android:color="@color/wifilist_emphasize_color" />

            <solid android:color="@color/wifilist_emphasize_color" />

            <corners android:topLeftRadius="@dimen/common_radius" android:topRightRadius="@dimen/common_radius" />
        </shape></item>
    <item android:state_pressed="false"><shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <stroke android:width="1px" android:color="@color/white" />

            <solid android:color="@color/white" />

            <corners android:topLeftRadius="@dimen/common_radius" android:topRightRadius="@dimen/common_radius" />
        </shape></item>

</selector>